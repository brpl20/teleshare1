{

  "manifest_version": 2,
  "name": "TeleShare",
  "version": "1.0",

  "description": "Shares current page with your Telegram Bot",

  "icons": {},

 "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["webhooker1.js"]
    }
  ],

  "commands": {
  "toggle-feature": {
    "suggested_key": {
      "default": "Alt+Shift+Q",
      "linux": "Ctrl+Shift+Q"
    },
    "description": "Send a 'toggle-feature' event"
  }
}





}
